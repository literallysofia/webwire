{
  "name": "webwire",
  "version": "1.0.0",
  "description": "A tool for automatic wireframe generation from websites.",
  "scripts": {
    "prebuild": "rm -rf dist && mkdir dist",
    "build": "tsc",
    "start": "npm run clean && node dist/run.js",
    "start:inspector": "rm -rf generated/data && node dist/run.js --inspector",
    "start:render": "rm -rf generated/images && node dist/run.js --render",
    "start:diff": "node dist/run.js --diff",
    "clean": "rm -rf generated && mkdir generated",
    "inspector": "node dist/inspector/index.js",
    "render": "node dist/render/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/literallysofia/feup-diss.git"
  },
  "author": "Sofia Silva",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/literallysofia/feup-diss/issues"
  },
  "homepage": "https://github.com/literallysofia/feup-diss#readme",
  "devDependencies": {
    "@types/chromedriver": "^2.38.0",
    "@types/cli-progress": "^3.7.0",
    "@types/command-line-args": "^5.0.0",
    "@types/js-yaml": "^3.12.4",
    "@types/jsdom": "^16.2.3",
    "@types/node": "^13.13.9",
    "@types/prettier": "^2.0.1",
    "@types/puppeteer": "^2.1.1",
    "@types/seedrandom": "^2.4.28",
    "@types/selenium-webdriver": "^4.0.9",
    "@types/svgo": "^1.3.3",
    "@types/xmlserializer": "^0.6.0",
    "npm": "^6.14.5",
    "typescript": "^3.9.3"
  },
  "dependencies": {
    "canvas": "^2.6.1",
    "chromedriver": "^80.0.2",
    "cli-progress": "^3.8.2",
    "colors": "^1.4.0",
    "command-line-args": "^5.1.1",
    "js-yaml": "^3.14.0",
    "jsdom": "^16.2.2",
    "json-diff": "^0.5.4",
    "json2typescript": "^1.2.5",
    "lorem-ipsum": "^2.0.3",
    "prettier": "^2.0.5",
    "puppeteer": "^2.1.1",
    "roughjs": "^4.3.1",
    "seedrandom": "^3.0.5",
    "selenium-webdriver": "^4.0.0-alpha.7",
    "svgo": "^1.3.2",
    "svgpath": "^2.2.3",
    "xmlserializer": "^0.6.1"
  }
}
