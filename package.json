{
  "name": "wireception",
  "version": "1.0.0",
  "description": "A tool for automatic wireframe generation from websites.",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "npm run build; node dist/index.js",
    "render": "npm run build; node dist/render.js; open wireframe.svg",
    "ui": "ts-node src/render.ts; open wireframe.svg",
    "browserify": "browserify dist/render.js -o bundle.js",
    "draw": "npm run build; npm run browserify; open index.html",
